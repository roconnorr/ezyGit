{
  "name": "ezygit",
  "productName": "ezygit",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "./.webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "scott",
  "license": "MIT",
  "config": {
    "forge": {
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/app.tsx",
                  "name": "main_window"
                }
              ]
            },
            "loggerPort": "9001"
          }
        ]
      ],
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ezygit"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.45",
    "@material-ui/core": "^4.4.2",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@electron-forge/cli": "^6.0.0-beta.33",
    "@electron-forge/maker-deb": "^6.0.0-beta.33",
    "@electron-forge/maker-rpm": "^6.0.0-beta.33",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.33",
    "@electron-forge/maker-zip": "^6.0.0-beta.33",
    "babel-loader": "^8.0.6",
    "electron": "6.0.10",
    "webpack": "^4.40.2"
  }
}
